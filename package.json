{
	"name": "chess-move-notes",
	"version": "0.3.0",
	"description": "Chess diagrams and move notation for Obsidian (maintained fork of Chesser).",
	"main": "main.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RichLewis007/Chess-Move-Notes.git"
	},
	"bugs": {
		"url": "https://github.com/RichLewis007/Chess-Move-Notes/issues"
	},
	"homepage": "https://github.com/RichLewis007/Chess-Move-Notes#readme",
	"scripts": {
		"build": "tsc -noEmit -skipLibCheck && vite build",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"postbuild": "node scripts/copy-manifest.mjs",
		"dev": "vite build --watch",
		"new-release": "node scripts/version-bump.mjs && git-cliff",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"check": "npm run lint && npm run format:check",
		"check:fix": "npm run lint:fix && npm run format",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,cjs,mjs}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{css,json,md,yml,yaml}": [
			"prettier --write"
		]
	},
	"keywords": [],
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@types/chess.js": "^0.13.7",
		"@types/node": "^22.15.2",
		"@vitest/coverage-v8": "^4.0.18",
		"builtin-modules": "^5.0.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.0.0",
		"globals": "^15.0.0",
		"husky": "^9.1.7",
		"jsdom": "^28.0.0",
		"lint-staged": "^16.2.7",
		"obsidian": "1.8.7",
		"prettier": "^3.0.0",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^6.3.3",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"chess.js": "^1.2.0",
		"chessground": "^9.1.1",
		"nanoid": "^5.1.5"
	}
}
